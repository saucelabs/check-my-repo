          "level": "error",
          "rule": {
            "type": "file-existence",
            "options": {
              "globsAny": ["README*"],
              "nocase": true
            }
          },
          "fix": {
            "type": "file-create",
            "options": {
              "file": "README.md",
              "replace": true,
              "text": {
                "url": "https://github.com/saucelabs/new-project/blob/main/README.md"
              }
            }
          },
          "policyInfo": "README should always be up-to-date",
          "policyUrl": "https://opensource.saucelabs.com/docs/"
        },
        "Changelog": {
          "level": "error",
          "rule": {
            "type": "file-existence",
            "options": {
              "globsAny": ["CHANGELOG*"],
              "nocase": true
            }
          },
          "policyInfo": "A log of all notable changes made to a project"
        },
        "Maintainers email": {
          "level": "error",
          "rule": {
            "type": "file-contents",
            "options": {
              "globsAll": [
                "MAINTAINERS*",
                "CODE-OWNERS*",
                "CODE_OWNERS*",
                ".github/MAINTAINERS*",
                ".github/CODEOWNERS*",
                ".github/CODE_OWNERS*"
              ],
              "content": ".+@.+\\..+",
              "flags": "i",
              "human-readable-content": "email address"
            }
          }
        },
        "License": {
          "level": "error",
          "rule": {
            "type": "file-existence",
            "options": {
              "globsAny": ["LICENSE*", "COPYING*"],
              "nocase": true
            }
          },
          "fix": {
            "type": "file-create",
            "options": {
              "file": "LICENSE",
              "replace": true,
              "text": {
                "url": "https://github.com/saucelabs/new-project/blob/main/LICENSE"
              }
            }
          },
          "policyInfo": "A repository must contain a permissive or weak copy-left license",
          "policyUrl": "https://opensource.saucelabs.com/docs/license-guide/"
        },
        "Do not have binaries files": {
          "level": "error",
          "rule": {
            "type": "file-type-exclusion",
            "options": {
              "type": ["**/*.exe", "**/*.dll", "!node_modules/**"]
            }
          },
          "policyInfo": "Binaries makes your repo heavy, slown and are hard to remove"
        },
        "Test directory": {
          "level": "error",
          "rule": {
            "type": "directory-existence",
            "options": {
              "globsAny": ["**/test*", "**/specs"],
              "nocase": true
            }
          },
          "policyInfo": "Tests creates a reliable repository"
        }
      }
    }
  },
